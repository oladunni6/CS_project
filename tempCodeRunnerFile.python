class Car:
   def __init__(self, color, model):
       self.color = color        
       self.__model = model      


   def move(self):
       return f"The {self.color} car is moving."


   def get_model(self):
       return self.__model       


my_car = Car("white", "Mercedes Benz")
print(my_car.color) 
print(my_car.get_model()) 
print(my_car.move())